<?xml version="1.0" encoding="UTF-8" ?>
<themeeditor>
	<author>leotheme</author>
	<name>Live Theme Editor</name>	
	<configs>
		<item>
			<block>header_style</block>
			<title>Header Style</title>
			<default>classic</default>
			<option>
				 <name>Default</name>
				 <id>header-default</id>
			</option>
			<option>
				 <name>Classic</name>
				 <id>header-classic</id>
			</option>
			<option>
				 <name>Stack: Left</name>
				 <id>header-stack-left</id>
			</option>
			<option>
				 <name>Stack: Right</name>
				 <id>header-stack-right</id>
			</option>
			<option>
				 <name>Stack: Center</name>
				 <id>header-stack-center</id>
			</option>
		</item>
	</configs>
</themeeditor>